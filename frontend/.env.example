# Firebase (Frontend)
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=
VITE_FIREBASE_MEASUREMENT_ID=

# Backend API base URL (Frontend)
VITE_API_URL=http://localhost:5000
# ========================================
# Firebase Configuration
# ========================================
# Get these from Firebase Console: https://console.firebase.google.com
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# ========================================
# Google Gemini AI API (Primary AI Provider)
# ========================================
# Get your FREE API key at: https://makersuite.google.com/app/apikey
# Gemini 1.5 Flash is used for AI trip planning and recommendations
# Free tier: 15 requests/minute, 1500 requests/day
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# ========================================
# AviationStack Flight Info API (Optional)
# ========================================
# Get your FREE API key at: https://aviationstack.com/
# Used for real-time flight information lookup
# Free tier: 100 requests/month
VITE_AVIATIONSTACK_KEY=your_aviationstack_key_here

# ========================================
# Application Environment
# ========================================
VITE_APP_ENV=development
